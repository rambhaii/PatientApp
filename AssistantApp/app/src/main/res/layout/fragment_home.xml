<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/whitep"
    tools:context=".Dashbord.ui.HomeFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_180sdp"
                    android:layout_below="@+id/header"
                    android:background="@color/whitep">

                    <androidx.viewpager.widget.ViewPager xmlns:android="http://schemas.android.com/apk/res/android"
                        android:id="@+id/pager"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_180sdp"></androidx.viewpager.widget.ViewPager>

                    <LinearLayout
                        android:id="@+id/image_count"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:background="#00000000"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:visibility="gone" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/chatbanner"></ImageView>

                    <TextView
                        android:id="@+id/chatButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:gravity="center"
                        android:background="@color/green"
                        android:text="Chat Now"
                        android:padding="10dp"
                        android:layout_marginBottom="100dp"
                        android:textColor="@color/whitep" />

                </RelativeLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="serif"
                    android:gravity="left"
                    android:paddingTop="10dp"
                    android:singleLine="true"
                    android:text="Find doctor in top specialities"
                    android:textAllCaps="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_12sdp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="10dp"
                    android:textStyle="bold"
                    android:visibility="visible" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">


                    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/tools"
                        xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/find_doctor"
                        android:layout_width="match_parent"

                        android:layout_height="wrap_content"

                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_weight="1"
                        android:background="@drawable/rectbacknew"
                        android:orientation="vertical">


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:elevation="@dimen/_2sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_1sdp"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                                    android:layout_width="@dimen/_70sdp"
                                    android:layout_height="@dimen/_70sdp"
                                    android:layout_centerInParent="true"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/_5sdp"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:layout_marginRight="@dimen/_5sdp"
                                    android:src="@drawable/finddoctors2"
                                    app:civ_border_color="@color/white"
                                    app:civ_border_width="1dp" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:layout_gravity="left"
                                    android:layout_marginTop="@dimen/_6sdp"
                                    android:background="@color/white"
                                    android:fontFamily="serif"
                                    android:gravity="center"
                                    android:maxLines="2"
                                    android:padding="@dimen/_1sdp"
                                    android:text="Find Doctor \n Near You"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_9sdp"
                                    android:textStyle="bold" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:layout_gravity="left"
                                    android:background="@color/white"
                                    android:fontFamily="serif"
                                    android:gravity="center"
                                    android:maxLines="2"
                                    android:padding="@dimen/_1sdp"
                                    android:text="Book video or in-clinic appointment"
                                    android:textColor="@color/background"
                                    android:textSize="@dimen/_9sdp"
                                    android:visibility="gone" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="left"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:background="@color/white"
                                    android:drawableRight="@drawable/arrow_right"
                                    android:drawablePadding="@dimen/_10sdp"
                                    android:fontFamily="serif"
                                    android:gravity="center"
                                    android:maxLines="1"
                                    android:padding="@dimen/_1sdp"
                                    android:text="FIND DOCTORS"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="@dimen/_9sdp"
                                    android:visibility="gone" />


                            </LinearLayout>


                        </RelativeLayout>


                    </LinearLayout>

                    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/tools"
                        xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/doctorsearch"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:layout_weight="1"
                        android:background="@drawable/rectbacknew"
                        android:orientation="vertical">


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:elevation="@dimen/_2sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_1sdp"
                                android:elevation="@dimen/_2sdp"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                                    android:layout_width="@dimen/_70sdp"
                                    android:layout_height="@dimen/_70sdp"
                                    android:layout_centerInParent="true"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/_5sdp"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:layout_marginRight="@dimen/_5sdp"
                                    android:src="@drawable/pingone"
                                    app:civ_border_color="@color/white"
                                    app:civ_border_width="1dp" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:layout_gravity="left"
                                    android:layout_marginTop="@dimen/_6sdp"
                                    android:background="@color/white"
                                    android:fontFamily="serif"
                                    android:gravity="center"
                                    android:maxLines="2"
                                    android:padding="@dimen/_1sdp"
                                    android:text="Doctor Search"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_9sdp"
                                    android:textStyle="bold" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:layout_gravity="left"
                                    android:background="@color/white"
                                    android:fontFamily="serif"
                                    android:gravity="center"
                                    android:maxLines="2"
                                    android:padding="@dimen/_1sdp"
                                    android:text="Consult online with a\n 24x7 specialist"
                                    android:textColor="@color/background"
                                    android:textSize="@dimen/_9sdp"
                                    android:visibility="gone" />


                                <TextView
                                    android:layout_width="match_parent"

                                    android:layout_height="wrap_content"
                                    android:layout_gravity="left"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:background="@color/white"
                                    android:drawableRight="@drawable/arrow_right"
                                    android:drawablePadding="@dimen/_10sdp"
                                    android:fontFamily="serif"
                                    android:gravity="center"
                                    android:maxLines="1"
                                    android:padding="@dimen/_1sdp"
                                    android:text="START Search "
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="@dimen/_9sdp"
                                    android:visibility="gone" />


                            </LinearLayout>


                        </RelativeLayout>


                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:orientation="horizontal">


                    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/tools"
                        xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_weight="1"
                        android:background="@drawable/rectbacknew"
                        android:orientation="vertical">


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:id="@+id/Consultdoctor"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_1sdp"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                                    android:layout_width="@dimen/_70sdp"
                                    android:layout_height="@dimen/_70sdp"
                                    android:layout_centerInParent="true"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/_5sdp"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:layout_marginRight="@dimen/_5sdp"
                                    android:src="@drawable/pingtwo"
                                    app:civ_border_color="@color/white"
                                    app:civ_border_width="1dp" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:layout_gravity="left"
                                    android:layout_marginTop="@dimen/_6sdp"
                                    android:background="@color/white"
                                    android:fontFamily="serif"
                                    android:gravity="center"
                                    android:maxLines="2"
                                    android:padding="@dimen/_1sdp"
                                    android:text="Consult with a Doctor"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_9sdp"
                                    android:textStyle="bold" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:layout_gravity="left"

                                    android:background="@color/white"
                                    android:fontFamily="serif"
                                    android:gravity="center"
                                    android:maxLines="2"
                                    android:padding="@dimen/_1sdp"
                                    android:text="Get lab test done at \n home"
                                    android:textColor="@color/background"
                                    android:textSize="@dimen/_9sdp"
                                    android:visibility="gone" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"

                                    android:layout_gravity="left"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:background="@color/white"
                                    android:drawableRight="@drawable/arrow_right"
                                    android:drawablePadding="@dimen/_10sdp"
                                    android:fontFamily="serif"
                                    android:gravity="center"
                                    android:maxLines="1"
                                    android:padding="@dimen/_1sdp"
                                    android:text="BOOK NOW"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="@dimen/_9sdp"
                                    android:visibility="gone" />


                            </LinearLayout>


                        </RelativeLayout>


                    </LinearLayout>

                    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/tools"
                        xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:layout_weight="1"
                        android:background="@drawable/rectbacknew"
                        android:orientation="vertical">


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:id="@+id/AllSymtoms"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"

                                android:layout_margin="@dimen/_1sdp"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                                    android:layout_width="@dimen/_70sdp"
                                    android:layout_height="@dimen/_70sdp"
                                    android:layout_centerInParent="true"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/_5sdp"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:layout_marginRight="@dimen/_5sdp"
                                    android:src="@drawable/docsappoffers"
                                    app:civ_border_color="@color/white"
                                    app:civ_border_width="1dp" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:layout_gravity="left"
                                    android:layout_marginTop="@dimen/_6sdp"
                                    android:background="@color/white"
                                    android:fontFamily="serif"
                                    android:gravity="center"
                                    android:maxLines="2"
                                    android:padding="@dimen/_1sdp"
                                    android:text="Fetch Doctor By symptoms"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_9sdp"
                                    android:textStyle="bold" />


                            </LinearLayout>

                        </RelativeLayout>

                    </LinearLayout>

                </LinearLayout>


                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recycler_view_doctor"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_above="@+id/checkoutlayout"
                    android:layout_below="@+id/toolbar"
                    android:nestedScrollingEnabled="false"
                    android:padding="@dimen/_5sdp"
                    android:scrollbars="vertical"
                    android:visibility="gone"
                    tools:context="example.javatpoint.com.recyclerviewlist.MainActivity" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/medicne_store"></ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="serif"
                    android:gravity="left"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingTop="@dimen/_5sdp"
                    android:singleLine="true"
                    android:text="Specialities"
                    android:textAllCaps="true"
                    android:layout_marginBottom="10dp"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold" />


                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recycler_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_above="@+id/checkoutlayout"
                    android:layout_below="@+id/toolbar"
                    android:nestedScrollingEnabled="false"
                    android:padding="@dimen/_5sdp"
                    android:scrollbars="vertical"
                    tools:context="example.javatpoint.com.recyclerviewlist.MainActivity" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/pythology"></ImageView>

                    <TextView
                        android:id="@+id/pyButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="60dp"
                        android:gravity="center"
                        android:background="@color/colorPrimary"
                        android:text="Search Pythology"
                        android:padding="10dp"
android:layout_marginRight="@dimen/_30sdp"
                        android:textColor="@color/whitep" />
                </RelativeLayout>

            </LinearLayout>

        </androidx.core.widget.NestedScrollView>

        <RelativeLayout
            android:id="@+id/header"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_45sdp"
            android:background="@color/colorPrimary"
            android:visibility="gone">

            <RelativeLayout
                android:id="@+id/editlocation"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_45sdp"
                android:layout_alignLeft="@+id/item"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_toLeftOf="@+id/item"
                android:gravity="center_vertical">


                <TextView
                    android:id="@+id/location"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableRight="@drawable/ic_arrow_drop_down_black_24dp"
                    android:drawablePadding="@dimen/_10sdp"
                    android:drawableTint="@color/white"
                    android:singleLine="true"
                    android:text="XXXXXXXXX"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_13sdp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/AddressLine"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/location"
                    android:singleLine="true"
                    android:text="CCCCCCCCCCCCCCCCCCCCCCC"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_12sdp" />

            </RelativeLayout>


            <LinearLayout
                android:id="@+id/item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:gravity="end"
                android:orientation="horizontal">


                <RelativeLayout
                    android:id="@+id/notification"
                    android:layout_width="@dimen/_35sdp"
                    android:layout_height="@dimen/_35sdp">


                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:padding="@dimen/_2sdp">


                        <ImageView
                            android:layout_width="@dimen/_33sdp"
                            android:layout_height="@dimen/_33sdp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:padding="@dimen/_5sdp"
                            android:src="@drawable/ic_notifications_black_24dp"
                            android:tint="@color/white" />

                        <LinearLayout
                            android:id="@+id/lired"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/rednew"
                                android:layout_width="@dimen/_5sdp"
                                android:layout_height="@dimen/_5sdp"
                                android:layout_alignParentRight="true"
                                android:tint="@color/red"
                                app:srcCompat="@drawable/purple_circle" />

                        </LinearLayout>


                    </RelativeLayout>


                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/share"
                    android:layout_width="@dimen/_35sdp"
                    android:layout_height="@dimen/_35sdp"

                    android:padding="@dimen/_2sdp">

                    <ImageView
                        android:layout_width="@dimen/_33sdp"
                        android:layout_height="@dimen/_33sdp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:padding="@dimen/_5sdp"
                        android:src="@drawable/ic_share_black_24dp"
                        android:tint="@color/white" />

                </RelativeLayout>

            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="@dimen/_5sdp"
                android:gravity="center_vertical"
                android:visibility="gone">

                <RelativeLayout
                    android:id="@+id/piclayout"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    app:srcCompat="@drawable/operator_circle">

                    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/userpic"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_centerInParent="true"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/customer_support"
                        app:civ_border_color="@color/black"
                        app:civ_border_width="1dp" />

                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/search"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/_1sdp"
                    android:layout_marginRight="@dimen/_1sdp"
                    android:layout_toLeftOf="@+id/balanceli"
                    android:layout_toRightOf="@+id/piclayout"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:background="@drawable/rect"
                        android:padding="@dimen/_6sdp">

                        <TextView
                            android:id="@+id/password"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@android:color/transparent"
                            android:hint="Search For Doctors And Medicine"
                            android:inputType="textPassword"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_11sdp" />

                    </RelativeLayout>


                    <TextView
                        android:id="@+id/name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/_28sdp"
                        android:fontFamily="@font/proximanovaregular"
                        android:gravity="left"

                        android:singleLine="true"
                        android:text=""
                        android:textColor="@color/black"
                        android:textSize="@dimen/_11sdp"
                        android:textStyle="bold"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/contentnumber"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/proximanovaregular"
                        android:gravity="left"
                        android:text=""
                        android:textColor="@color/black"
                        android:textSize="@dimen/_9sdp"
                        android:visibility="gone" />

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/balanceli"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/Logout_tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:fontFamily="@font/proximanovaregular"
                        android:gravity="left"
                        android:singleLine="true"
                        android:text="     Logout       "
                        android:textColor="@color/red"
                        android:textSize="@dimen/_11sdp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/balanceamount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/proximanovaregular"
                        android:gravity="left"
                        android:text="Rs.  0.00"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_9sdp"
                        android:textStyle="bold"
                        android:visibility="gone" />

                </LinearLayout>


